{% include 'header.j2' %}
        {% include 'search.j2' %}
        {% if message_range and total_count -%}
        <table align='center' width='85%' border='0' cellpadding='10'>
            <tr>
                <td bgcolor='#ffffff'>Showing {{ message_range }} of {{ total_count }} results</td>
            </tr>
            <tr>
                {% if show_prev -%}<td width='80px' align='left'><a href='{{ prev_page_url }}'>&lt Prev Page</a></td>{% endif -%}
                <td width='*'>&nbsp;</td>
                {% if show_next -%}<td width='80px' align='right'><a href='{{ next_page_url }}'>Next Page &gt;</a></td>{% endif -%}
            </tr>
        </table>
		<table id="striped" align='center' width='85%' border='0' cellpadding='5'>
            <tr>
                <th>Team Name</th>
                <th>Channel Name</th>
                <th>Username</th>
                <th>Date</th>
                <th>Time</th>
                <th>Text</th>
                <th>&nbsp;</th>
            </tr>
            {% for message in messages -%}
            <tr>
                <td>{{ message["team_name"] }}</td>
                <td>{{ message["channel_name"] }}</td>
                <td>{{ message["username"] }}</td>
                <td>{{ message["date"] }}</td>
                <td>{{ message["time"] }}</td>
                <td class='wrappable'>{{ message["text"] }}</td>
                <td><a href='/context/{{ message["id"] }}/{{ q }}'>See in context</a></td>
            </tr>
            {% endfor -%}
        </table>
        <table align='center' width='85%' border='0' cellpadding='10'>
            <tr>
                <td bgcolor='#ffffff'>Showing {{ message_range }} of {{ total_count }} results</td>
            </tr>
            <tr>
                {% if show_prev -%}<td width='80px' align='left'><a href='/?q={{ q }}&page={{ page - 1 }}'>&lt Prev Page</a></td>{% endif -%}
                <td width='*'>&nbsp;</td>
                {% if show_next -%}<td width='80px' align='right'><a href='/?q={{ q }}&page={{ page + 1 }}'>Next Page &gt;</a></td>{% endif -%}
            </tr>
        </table>
        {% endif -%}
{% include 'footer.j2' %}
